{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "publisher": "sshcrack",
    "longDescription": "This app uses RSA to encrypt messages being sent over the tor network. There is no central server as this app is centralized.",
    "category": "SocialNetworking",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "enkrypton",
  "mainBinaryName": "enkrypton",
  "version": "1.0.2",
  "identifier": "me.sshcrack.enkrypton",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "enkrypton",
        "width": 800,
        "height": 600,
        "url": "src/app/index.html",
        "visible": false,
        "label": "main",
        "useHttpsScheme": true
      },
      {
        "width": 400,
        "height": 200,
        "decorations": false,
        "url": "index.html",
        "label": "splashscreen",
        "center": true,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}